<h1>Project Details</h1>

<h2>
    <%= project.name %>
</h2>
<p><strong>Author:</strong>
    <%= project.author %>
</p>
<p><strong>Description:</strong>
    <%= project.description %>
</p>

<h3>Issues:</h3>
<ul>
    <% project.issues.forEach(function(issue) { %>
        <li>
            <strong>
                <%= issue.title %>
            </strong>
            <p>
                <%= issue.description %>
            </p>
        </li>
        <% }); %>
</ul>